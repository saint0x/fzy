const ITERATIONS: i32 = 8000000

fn workload() -> i32 {
    let a: i32 = 3
    let b: i32 = 17
    let mut i: i32 = 0
    let mut acc: i32 = 0

    while i < ITERATIONS {
        let c = a + b
        let d = c - a
        acc = (acc + (d % 251)) % 251
        i += 1
    }

    return acc
}

fn main() -> i32 {
    return workload()
}
