use cap.net;
use cap.time;

fn listen_public() -> i32 {
    let conn = net.connect()
    defer close(conn)
    time.now()
    return 0
}

fn route_health() -> i32 {
    let conn = net.connect()
    defer close(conn)
    return 0
}

fn route_users() -> i32 {
    let conn = net.connect()
    defer close(conn)
    return 0
}
