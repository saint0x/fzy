use core.fs;
use core.time;
fn record_req() -> i32 {
    let h = fs.open() defer close(h) log.info("metrics.req", log.fields2("component", "metrics", "phase", "req")) time.now() return 0
}
fn record_err() -> i32 {
    let h = fs.open() defer close(h) log.warn("metrics.err", log.fields2("component", "metrics", "phase", "err")) time.now() return 0
}
