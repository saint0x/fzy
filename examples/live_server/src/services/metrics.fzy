use cap.fs;
use cap.time;

fn record_req() -> i32 {
    let h = fs.open()
    defer close(h)
    metrics.counter()
    metrics.gauge()
    time.now()
    return 0
}

fn record_err() -> i32 {
    let h = fs.open()
    defer close(h)
    metrics.counter()
    metrics.histogram()
    time.now()
    return 0
}
