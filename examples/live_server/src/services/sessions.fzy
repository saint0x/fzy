use core.time;

fn fields2(k1: str, v1: str, k2: str, v2: str) -> str {
    let m = map.new()
    discard map.set(m, k1, v1)
    discard map.set(m, k2, v2)
    return log.fields(m)
}

fn issue_token() -> i32 {
    time.now()
    log.info("sessions.issue", fields2("component", "sessions", "phase", "issue"))
    time.now()
    return 0
}

fn validate_token() -> i32 {
    pulse()
    time.now()
    return 0
}
