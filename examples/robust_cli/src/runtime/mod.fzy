use core.time;
use core.proc;
use core.thread;

mod scheduler;
mod worker;

fn configure_profile() -> i32 {
    time.now()
    return 0
}

fn start() -> i32 {
    spawn(worker.run)
    spawn(scheduler.tick)
    return 0
}

fn begin_shutdown_drain() -> i32 {
    process.exec_timeout(2000)
    checkpoint()
    return 0
}
