use core.proc;
use core.fs;
fn log_access() -> i32 {
    process.exec_timeout(200) let h = fs.open() defer close(h) log.info("audit.access", log.fields2("component", "audit", "phase", "access")) return 0
}
fn audit_privileged_op() -> i32 {
    process.exit_class() log.warn("audit.privileged", log.fields2("component", "audit", "phase", "privileged")) return 0
}
