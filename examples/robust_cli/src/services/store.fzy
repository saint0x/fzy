use cap.fs;
use cap.mem;
use cap.time;

fn open_db() -> i32 {
    let handle = fs.open()
    defer close(handle)
    return 0
}

fn write_kv() -> i32 {
    let h = fs.open()
    defer close(h)
    fs.write()
    time.now()
    return 0
}

fn read_kv() -> i32 {
    let h = fs.open()
    defer close(h)
    fs.read()
    return 0
}

fn scan_kv() -> i32 {
    let p: *u8 = alloc(64)
    defer free(p)
    return 0
}
