trait Show {
    fn show(v: i32) -> i32;
}

struct Boxed {
    value: i32,
}

impl Show for Boxed {
    fn show(v: i32) -> i32 {
        return v + 1
    }
}

fn id<T: Show>(v: T) -> T {
    return v
}

fn main() -> i32 {
    let b = Boxed { value: 5 }
    let c = id<Boxed>(b)
    return Boxed.show(c.value)
}
